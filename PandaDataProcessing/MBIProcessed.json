{
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-08-01/Microsoft.DataFactory.Table.json",
  "name": "MBIProcessed",
  "properties": {
    "type": "AzureBlob",
    "linkedServiceName": "StorageLinkedService",
    "structure": [
      {
        "name": "StudyID",
        "type": "String"
      },
      {
        "name": "Tag",
        "type": "String"
      },
      {
        "name": "TRT",
        "type": "String"
      },
      {
        "name": "Rep",
        "type": "String"
      },
      {
        "name": "ObservationDate",
        "type": "String"
      },
      {
        "name": "HasMBI",
        "type": "String"
      }
    ],
    "typeProperties": {
      "folderPath": "hdi/processed/mbi/",
      "format": {
        "type": "TextFormat",
        "columnDelimiter": ","
      }
    },
    "availability": {
      "frequency": "Hour",
      "interval": 1
    }
  }
}